#!/usr/bin/env sh

# Exit, if return non-zero or use an undefined variable.
set -eu

cd "$(dirname "$0")/.."

# Variables
# Variables
GIT_REPO_URL=https://github.com/upx/upx.git

git ls-remote --tags --refs $GIT_REPO_URL | sed 's/[\t ]\+/\t/g' | cut -f2 | sed 's|refs/tags/v||g' | sort -t. -k 1.1,1n -k 2,2n -k 3,3n -k 4,4n
